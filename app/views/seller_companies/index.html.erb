<p id="notice"><%= notice %></p>

<h1>Listing Seller Companies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seller_companies.each do |seller_company| %>
      <tr>
        <td><%= link_to 'Show', seller_company %></td>
        <td><%= link_to 'Edit', edit_seller_company_path(seller_company) %></td>
        <td><%= link_to 'Destroy', seller_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller company', new_seller_company_path %>
